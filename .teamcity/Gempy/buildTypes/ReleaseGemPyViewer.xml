<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="f46e0551-c5cf-4249-97e1-38bb3298a1b6" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2025.3/project-config.xsd">
  <name>Release GemPy Viewer</name>
  <description />
  <settings ref="ReleasePythonPackage">
    <disabled-settings>
      <setting-ref ref="Git_Tagging" />
    </disabled-settings>
    <parameters>
      <param name="env.org/repo" value="gempy-project/gempy_viewer" />
    </parameters>
    <build-runners>
      <runner id="Git_Tagging" name="Git Tagging" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[whoami
echo $HOME

git config --global credential.helper "store --file=/home/leguark/.git-credentials"

GIT_TRACE=1 GIT_CURL_VERBOSE=1 git tag v%env.PACKAGE_VERSION%
git push origin v%env.PACKAGE_VERSION%]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="GempyEngine_HttpsGithubComGempyProjectGempyViewerRefsHeadsMain" />
    </vcs-settings>
  </settings>
</build-type>

